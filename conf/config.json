{
  "logs": "/var/log/pufferpanel",
  "panel": {
    "database": {
      "dialect": "sqlite3",
      "url": "file:/var/lib/pufferpanel/database.db?cache=shared"
    },
    "web": {
      "files": "/var/www/pufferpanel"
    }
  },
  "daemon": {
    "data": {
      "cache": "/var/lib/pufferpanel/cache",
      "servers": "/var/lib/pufferpanel/servers"
    }
  },
  "web": {
  "host": "127.0.0.1:__PORT__"
  }
}
